{
  "name": "vega",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "app": "^0.1.0",
    "browser-window": "^0.4.0",
    "electron": "^3.0.5",
    "electron-builder": "^20.28.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.5"
